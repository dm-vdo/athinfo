# $Id: Makefile.in,v 1.3 1999-09-15 23:56:32 danw Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@
sysconfdir=@sysconfdir@

CC=@CC@
DEFS=@DEFS@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@ ${WARN_CFLAGS} ${ERROR_CFLAGS}
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
ALL_CFLAGS=${CPPFLAGS} ${CFLAGS} ${DEFS}

all:

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	${top_srcdir}/mkinstalldirs ${DESTDIR}${sysconfdir}/bash_completion.d
	${INSTALL_PROGRAM} athinfo ${DESTDIR}${bindir}
	${INSTALL} -m 444 ${srcdir}/athinfo.1 ${DESTDIR}${mandir}/man1
	${INSTALL} -m 644 ${srcdir}/bash_completion ${DESTDIR}${sysconfdir}/bash_completion.d/athinfo

clean:

distclean: clean
	rm -f config.cache config.log config.status Makefile
